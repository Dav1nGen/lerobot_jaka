{
    "batch_size": 16,
    "num_workers": 4,
    "steps": 5000,
    "log_freq": 10,
    "eval_freq": 1000,
    "save_freq": 1000,
    "save_checkpoint": true,
    "seed": 2,
    "resume": false,
    "output_dir":"/home/joysonrobot/output_dir",
