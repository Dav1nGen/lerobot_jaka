{
  "mode": "record",
  "device": "cuda",
  "env": {
    "name": "real_robot",
    "fps": 60,
    "processor": {
      "reset": {
        "reset_time_s": 5.0,
        "control_time_s": 20.0,
        "terminate_on_success": true
      },
      "gripper": {
        "use_gripper": false
      }
    },
    "robot": {
      "type": "jakaS12",
      "id": "JakaS12",
      "arm_ip": "192.168.1.5",
      "sucker_ip": "192.168.1.8",
      "sucker_port": 502,
      "coils_address": 8,
      "cameras": {
        "head": {
          "type": "opencv",
          "index_or_path": 4,
          "width": 640,
          "height": 480,
          "fps": 60,
          "color_mode": "rgb"
        },
        "wrist": {
          "type": "opencv",
          "index_or_path": 10,
          "width": 640,
          "height": 480,
          "fps": 60,
          "color_mode": "rgb"
        }
      }
    },
    "teleop": {
      "type": "jakaS12_leader",
      "arm_ip": "192.168.1.3",
      "drag_friction_compensation_gain": "(80, 80, 80, 80, 80, 80)",
      "is_block": false,
      "joint_speed": 3,
      "joint_acc": 3,
      "use_gripper": false
    }
  },

  "dataset": {
    "repo_id": "Dav1nGen/reward_classifier_data1",
    "root": "/home/joysonrobot/lerobot_dataset/reward_classifier_data1",
    "task": "reward_classifier_task",
    "num_episodes_to_record": 20,
    "replay_episode": null,
    "push_to_hub": false
  },

  "policy": {
    "type": "reward_classifier",
    "model_name": "helper2424/resnet10",
    "model_type": "cnn",
    "num_cameras": 2,
    "num_classes": 2,
    "hidden_dim": 256,
    "dropout_rate": 0.1,
    "learning_rate": 1e-4,
    "device": "cuda",
    "use_amp": true,
    "input_features": {
      "observation.images.front": {
        "type": "VISUAL",
        "shape": [3, 128, 128]
      },
      "observation.images.side": {
        "type": "VISUAL",
        "shape": [3, 128, 128]
      }
    }
  }
}